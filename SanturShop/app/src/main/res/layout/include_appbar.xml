<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout    xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:id="@+id/some_appbar_constr"    android:layout_width="match_parent"    android:layout_height="55dp"    android:background="@color/primary"    app:layout_constraintEnd_toEndOf="parent"    app:layout_constraintStart_toStartOf="parent"    app:layout_constraintTop_toTopOf="parent">    <View        android:id="@+id/some_addbar_view_back"        android:layout_width="48dp"        android:layout_height="55dp"        android:visibility="visible"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        android:foreground="?android:attr/selectableItemBackground"/>    <!--        android:background="@drawable/btn_circle_base_color"-->    <!--        android:foreground="?android:attr/selectableItemBackground"--><!--    android:layout_width="16dp"--><!--    android:layout_height="16dp"-->    <ImageView        android:id="@+id/some_appbar_img_back"        android:layout_width="16dp"        android:layout_height="16dp"        android:layout_marginStart="16dp"        android:visibility="visible"        android:src="@drawable/ic_back_additionally"        android:contentDescription="@drawable/ic_back_additionally"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"/>    <TextView        android:id="@+id/some_appbar_title"        style="@style/Title_Bold16"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:paddingEnd="70dp"        android:text="Some Title"        android:maxLines="1"        android:textColor="@color/white"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintStart_toEndOf="@+id/some_appbar_img_back"        app:layout_constraintTop_toTopOf="parent">    </TextView>    <TextView        android:id="@+id/some_appbar_btn_clear"        style="@style/Body_Regular14"        android:textColor="@color/white"        android:layout_width="wrap_content"        android:layout_height="match_parent"        android:gravity="center"        android:text="Some button"        android:layout_marginEnd="16dp"        android:clickable="true"        android:focusable="true"        android:visibility="invisible"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintTop_toTopOf="parent"        >    </TextView>    <View        android:id="@+id/some_appbar_view_search"        android:layout_width="48dp"        android:layout_height="55dp"        android:visibility="visible"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintTop_toTopOf="parent"        android:foreground="?android:attr/selectableItemBackground"/>    <ImageView        android:id="@+id/some_appbar_btn_search"        android:layout_width="24dp"        android:layout_height="24dp"        android:layout_marginEnd="16dp"        android:src="@drawable/ic_search"        android:visibility="visible"        app:layout_constraintBottom_toBottomOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintTop_toTopOf="parent">    </ImageView></androidx.constraintlayout.widget.ConstraintLayout>